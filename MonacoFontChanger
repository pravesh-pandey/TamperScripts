// ==UserScript==
// @name         MonacoFontChanger
// @version      1.1
// @description  try to take over the world!
// @author       https://www.linkedin.com/in/pravesh25/
// @match        http://*
// @match        https://*
// @grant        none
// ==/UserScript==

(function() {
    'use strict';
    const element = document.querySelectorAll('*'); // Select all elements
    element.forEach(e => {
        e.style.fontFamily='Monaco';
    });
})();
